{
  "name": "Goto Github Repo",
  "description" : "Fast access to github repos you have visited",
  "version": "0.3",
  "background": {
    "scripts": [
      "lodash.min.js",
      "helper.js",
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "history",
    "storage",
    "tabs"
  ],
  "omnibox": { "keyword" : "gh" },
  "icons": {
    "128": "images/logo.png",
    "16": "images/octo.png"
  },
  "manifest_version": 2
}
